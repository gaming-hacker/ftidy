C     MAXIMUM POSSIBLE CONTINUATION CARDS - ACTUAL LIMIT IS SET BY
C      *MAXC CARD (DEFAULT=19)
      INTEGER NQCNTS , MXDO, MXPLBL
      PARAMETER (NQCNTS=60)
C
C     MAXIMUM NUMBER OF NESTED DO-LOOPS
      PARAMETER (MXDO=80)
C
C     MAXIMUM NUMBER OF STATEMENT LABELS
      PARAMETER (MXPLBL=3500)
C
      COMMON LDEF(MXPLBL), LOCDEF(MXPLBL), LREF(MXPLBL), NEWNUM(MXPLBL),
     1 NNMKLS(MXPLBL), IOUTN(256)
C
C
      COMMON /MISCAL/ KEND(3), KBUFF(80), JINT((NQCNTS+1)*80), JOB(80),
     1 IOUT(66*(NQCNTS+1)+6), SERIAL(8), LCPY, KOL73(3), NTEMP(5)
      CHARACTER*2 KEND, KBUFF, JINT, JOB, KIM(80,NQCNTS+1), IOUT, SERIAL
     1 ,LCPY, KOL73, NTEMP
      EQUIVALENCE (JINT(1),KIM(1,1))
C
      DOUBLE PRECISION L772
      COMMON /MISC/ L772
     1 ,ICOL       ,IFIR       ,INDENT     ,IPASS      ,IQUIT
     2 ,ISTAR      ,JCOL
     3 ,JMAX       ,JUST       ,K72        ,KBKCOK     ,KFSPL
     4 ,KB15       ,KCTCTL     ,KD15       ,KD79       ,KDTRAN
     5 ,KHLOG      ,KHTRAN     ,KILI(8)    ,KOUNT      ,KPRIN
     6 ,KPUN       ,L25        ,LAST       ,LDOS(MXDO) ,LERR
     7 ,LFIR       ,LINE       ,LQUAL
     8 ,MANSI      ,MCOL       ,MCOM       ,MCONT      ,MDEB
     9 ,MEOF       ,MILDO      ,MLBL       ,MLGC       ,MLIST
     X ,MP2        ,MPAGE      ,MPRIN      ,MPUN       ,MREF
     X ,MRIT       ,MSER       ,MSKP       ,MSTOP      ,MXREF
     X ,MXRGHT     ,MCASE      ,MNDOO      ,MFMTB      ,MAXCNT
     X ,NBLC       ,NCD        ,NDEF       ,NDOS       ,NFEND
     X ,NINS       ,NLHTRN     ,NMSG       ,NPAGE      ,NPAR
     X ,NPUN       ,NREC       ,NREF       ,NROUT      ,NRT1
     X ,NRT2       ,NTRAN      ,NXEQ
C
      EQUIVALENCE (KILI(1),KLASS), (KILI(2),JTYPE), (KILI(3),L15),
     1 (KILI(4),IMAX), (KILI(5),MTRAN), (KILI(6),NXRF), (KILI(7),MEX),
     2 (KILI(8),ICOLSV)
C
C     COMMON /ALPHA/ KBL,KDIG(10),KABC(26),KSPK(14),KBL2,KLR2,KLP2,KRP2
      COMMON /ALPHA/ KBL,KDIG(10),KABC(26),KSPK(15),KBL2,KLR2,KLP2,KRP2
     1 ,KERM, KTAB, KAMPR, KAT, KCTCHR, KDEL1, KDEL2, KAPSTR, KALMRK,
     2 KALTRN, KBLCMT
      CHARACTER*2 KBL,KDIG,KABC,KSPK,KBL2,KLR2,KLP2,KRP2,KERM,KTAB,KAMPR
     1, KAT, KCTCHR, KDEL1, KDEL2, KAPSTR, KALMRK, KALTRN, KBLCMT
